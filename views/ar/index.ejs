    <script>
      randSlides();
      var splide = new Splide('.splide', {
        heightRatio: 0.3,
        autoHeight: true,
        autoplay: true,
        interval: 5000,
        lazyLoad: true,
        rewind: true,
        pauseOnHover: true,
        pauseOnFocus: true,
        type: 'fade',
        perPage: 1,
        direction: 'rtl'
      }).mount();
      //change ul li order randomly
      function randSlides() {
        var ul = document.querySelector('.splide__list');
        var li = document.querySelectorAll('.splide__slide');
        var liArray = Array.prototype.slice.call(li, 0);
        liArray.sort(function () { return 0.5 - Math.random() });
        ul.appendChild(liArray[0]);
        ul.appendChild(liArray[1]);
        ul.appendChild(liArray[2]);
        ul.appendChild(liArray[3]);
      }


