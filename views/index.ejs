      randSlides();

      $('#myCarousel').on('slide.bs.carousel', function () {
        var $nextImage = $('.active.carousel-item', this).next('.carousel-item').find('img');
        $nextImage.attr('src', $nextImage.data('lazy-load-src'));
      });
      new Splide('.splide', {
        heightRatio: 0.3,
        autoHeight: true,
        autoplay: true,
        interval: 5000,
        lazyLoad: true,
        rewind: true,
        type: 'fade',
        perPage: 1,
        pauseOnHover: true
      }).mount();

      //change ul li order randomly
      function randSlides() {
        var ul = document.querySelector('.splide__list');
        var li = document.querySelectorAll('.splide__slide');
        var liArray = Array.prototype.slice.call(li, 0);
        liArray.sort(function () { return 0.5 - Math.random() });
        ul.appendChild(liArray[0]);
        ul.appendChild(liArray[1]);
        ul.appendChild(liArray[2]);
        ul.appendChild(liArray[3]);
      }


